{
  "name": "attila",
  "description": "Ghost Theme",
  "demo": "https://attila.zutrinken.com",
  "version": "2.1.1",
  "engines": {
    "ghost": ">=3.0.0",
    "ghost-api": "v3"
  },
  "license": "MIT",
  "author": {
    "name": "Peter Amende",
